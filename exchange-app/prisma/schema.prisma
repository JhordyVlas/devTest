// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./local.db"
}

model Symbols{
  id Int @id @default(autoincrement())
  name String @unique
  state String
}

model Orders {
  id BigInt @id
  symbol String
  side String
}